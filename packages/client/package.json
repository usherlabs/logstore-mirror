{
	"name": "@logsn/client",
	"version": "0.0.3",
	"description": "The Log Store Network Client",
	"repository": "https://github.com/usherlabs/logstore.git",
	"author": "Victor Shevtsov <victor.shevtsov@usher.so>",
	"license": "Apache-2.0",
	"private": false,
	"types": "./dist/types/src/index.d.ts",
	"main": "./dist/src/exports-commonjs.js",
	"browser": "./dist/logstore-client.web.js",
	"exports": {
		"browser": "./dist/logstore-client.web.js",
		"default": {
			"import": "./dist/src/exports-esm.mjs",
			"require": "./dist/src/exports-commonjs.js"
		}
	},
	"scripts": {
		"postbuild": "bash copy-package.sh && bash fix-esm.sh && cpy ./node_artifacts ./dist",
		"format": "prettier --write .",
		"test": "dotenv -e .env jest --",
		"build": "tsc --build ./tsconfig.node.json",
		"build:modules": "tsc --build ./tsconfig.node.modules.json",
		"build-production": "pnpm clean; NODE_ENV=production pnpm build && pnpm build-browser-production",
		"build-browser-development": "NODE_ENV=development webpack --mode=development --progress",
		"build-browser-production": "NODE_ENV=production webpack --mode=production --progress",
		"clean": "jest --clearCache || true; rm -rf dist  *.tsbuildinfo node_modules/.cache || true",
		"test-browser": "pnpm build-browser-production && concurrently -k --success=first 'node ./test/browser/server.js' 'sleep 5 && nightwatch --headless --verbose --timeout=30000 --config=nightwatch.conf.js ./test/browser/browser.js'",
		"test-exports": "cd test/exports && npm run link && tsc --noEmit --project ./tsconfig.json && npm test",
		"browser": "node ./test/browser/server.js",
		"modules": "bash ./create-modules.sh"
	},
